<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Evaluation">

    <ImageView
        android:id="@+id/viewblue2"
        android:layout_width="833dp"
        android:layout_height="match_parent"
        android:contentDescription="@string/bgdesc"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.469"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/blueview" />

    <ImageView
        android:id="@+id/onestaranchor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="23dp"
        android:layout_marginTop="12dp"
        android:contentDescription="@string/onestar"
        app:layout_constraintStart_toEndOf="@+id/nickname2"
        app:layout_constraintTop_toBottomOf="@+id/anchorevalutionbox"
        app:srcCompat="@drawable/star1" />

    <ImageView
        android:id="@+id/twostaranchor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:contentDescription="@string/twostars"
        app:layout_constraintStart_toEndOf="@+id/onestaranchor"
        app:layout_constraintTop_toBottomOf="@+id/anchorevalutionbox"
        app:srcCompat="@drawable/star2" />

    <ImageView
        android:id="@+id/threestaranchor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:contentDescription="@string/threestars"
        app:layout_constraintStart_toEndOf="@+id/twostaranchor"
        app:layout_constraintTop_toBottomOf="@+id/anchorevalutionbox"
        app:srcCompat="@drawable/star2" />

    <ImageView
        android:id="@+id/fourstaranchor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:contentDescription="@string/fourstars"
        app:layout_constraintStart_toEndOf="@+id/threestaranchor"
        app:layout_constraintTop_toBottomOf="@+id/anchorevalutionbox"
        app:srcCompat="@drawable/star2" />

    <ImageView
        android:id="@+id/fivestaranchor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:contentDescription="@string/fivestars"
        app:layout_constraintStart_toEndOf="@+id/fourstaranchor"
        app:layout_constraintTop_toBottomOf="@+id/anchorevalutionbox"
        app:srcCompat="@drawable/star2" />

    <ImageButton
        android:id="@+id/backbutton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="16dp"
        android:background="#00FFFFFF"
        android:contentDescription="@string/back"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/back"
        tools:ignore="SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/playlistevalutionbox"
        android:layout_width="332dp"
        android:layout_height="120dp"
        android:layout_marginTop="28dp"
        android:background="#ABFFE7"
        android:cursorVisible="true"
        android:ellipsize="none"
        android:ems="10"
        android:fontFamily="monospace"
        android:gravity="top|left"
        android:hint="@string/playlisthint"
        android:inputType="textMultiLine|text"
        android:maxLines="5"
        android:textColor="#242A95"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.455"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/anchorevalutionbox"
        android:layout_width="332dp"
        android:layout_height="120dp"
        android:layout_marginTop="80dp"
        android:background="#ABFFE7"
        android:cursorVisible="true"
        android:ems="10"
        android:fontFamily="monospace"
        android:gravity="top|left"
        android:hint="@string/anchorhint"
        android:inputType="textMultiLine|text"
        android:maxLines="5"
        android:textColor="#242A95"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.455"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nickname" />

    <EditText
        android:id="@+id/nickname2"
        android:layout_width="170dp"
        android:layout_height="48dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="92dp"
        android:background="#FFB5FBE7"
        android:cursorVisible="true"
        android:ems="10"
        android:fontFamily="monospace"
        android:hint="@string/nickhint"
        android:inputType="textPersonName"
        android:maxLength="14"
        android:maxLines="1"
        android:textColor="#242A95"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@+id/okbutton2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/anchorevalutionbox" />

    <ImageButton
        android:id="@+id/okbutton2"
        android:layout_width="67dp"
        android:layout_height="63dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="44dp"
        android:background="#00FFFFFF"
        android:contentDescription="@string/ok"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/anchorevalutionbox"
        app:srcCompat="@drawable/done"
        app:tint="#43FB00"
        tools:ignore="SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/nickname"
        android:layout_width="170dp"
        android:layout_height="48dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="12dp"
        android:background="#FFB5FBE7"
        android:cursorVisible="true"
        android:ems="10"
        android:fontFamily="monospace"
        android:hint="@string/nickhint"
        android:inputType="textPersonName"
        android:maxLength="14"
        android:maxLines="1"
        android:textColor="#242A95"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/playlistevalutionbox"
        tools:ignore="DuplicateSpeakableTextCheck" />

    <ImageButton
        android:id="@+id/okbutton"
        android:layout_width="67dp"
        android:layout_height="63dp"
        android:layout_marginStart="92dp"
        android:layout_marginTop="60dp"
        android:background="#00FFFFFF"
        android:contentDescription="@string/ok"
        app:layout_constraintStart_toEndOf="@+id/nickname"
        app:layout_constraintTop_toBottomOf="@+id/playlistevalutionbox"
        app:srcCompat="@drawable/done"
        app:tint="#43FB00"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/onestar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:contentDescription="@string/onestar"
        app:layout_constraintEnd_toStartOf="@+id/twostars"
        app:layout_constraintTop_toBottomOf="@+id/playlistevalutionbox"
        app:srcCompat="@drawable/star1" />

    <ImageView
        android:id="@+id/twostars"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:contentDescription="@string/twostars"
        app:layout_constraintEnd_toStartOf="@+id/threestars"
        app:layout_constraintTop_toBottomOf="@+id/playlistevalutionbox"
        app:srcCompat="@drawable/star2" />

    <ImageView
        android:id="@+id/threestars"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:contentDescription="@string/threestars"
        app:layout_constraintEnd_toStartOf="@+id/fourstars"
        app:layout_constraintTop_toBottomOf="@+id/playlistevalutionbox"
        app:srcCompat="@drawable/star2" />

    <ImageView
        android:id="@+id/fourstars"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:contentDescription="@string/fourstars"
        app:layout_constraintEnd_toStartOf="@+id/fivestars"
        app:layout_constraintTop_toBottomOf="@+id/playlistevalutionbox"
        app:srcCompat="@drawable/star2" />

    <ImageView
        android:id="@+id/fivestars"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="524dp"
        android:layout_marginTop="12dp"
        android:layout_marginBottom="24dp"
        android:contentDescription="@string/fivestars"
        app:layout_constraintBottom_toTopOf="@+id/okbutton"
        app:layout_constraintStart_toStartOf="@+id/viewblue2"
        app:layout_constraintTop_toBottomOf="@+id/playlistevalutionbox"
        app:srcCompat="@drawable/star2" />

</androidx.constraintlayout.widget.ConstraintLayout>