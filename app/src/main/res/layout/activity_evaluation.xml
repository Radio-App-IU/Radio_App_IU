<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Evaluation">

    <ImageView
        android:id="@+id/viewblue2"
        android:layout_width="833dp"
        android:layout_height="match_parent"
        android:contentDescription="@string/bgdesc"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.469"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/blueview" />

    <ImageView
        android:id="@+id/onestaranchor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="23dp"
        android:layout_marginTop="12dp"
        android:contentDescription="@string/onestar"
        app:layout_constraintEnd_toStartOf="@+id/twostaranchor"
        app:layout_constraintStart_toEndOf="@+id/nickname2"
        app:layout_constraintTop_toBottomOf="@+id/anchorevalutionbox"
        app:srcCompat="@drawable/star1" />

    <ImageView
        android:id="@+id/twostaranchor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:contentDescription="@string/twostars"
        app:layout_constraintEnd_toStartOf="@+id/threestaranchor"
        app:layout_constraintStart_toEndOf="@+id/onestaranchor"
        app:layout_constraintTop_toBottomOf="@+id/anchorevalutionbox"
        app:srcCompat="@drawable/star2" />

    <ImageView
        android:id="@+id/threestaranchor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:contentDescription="@string/threestars"
        app:layout_constraintEnd_toStartOf="@+id/fourstaranchor"
        app:layout_constraintStart_toEndOf="@+id/twostaranchor"
        app:layout_constraintTop_toBottomOf="@+id/anchorevalutionbox"
        app:srcCompat="@drawable/star2" />

    <ImageView
        android:id="@+id/fourstaranchor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:contentDescription="@string/fourstars"
        app:layout_constraintEnd_toStartOf="@+id/fivestaranchor"
        app:layout_constraintStart_toEndOf="@+id/threestaranchor"
        app:layout_constraintTop_toBottomOf="@+id/anchorevalutionbox"
        app:srcCompat="@drawable/star2" />

    <ImageView
        android:id="@+id/fivestaranchor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:contentDescription="@string/fivestars"
        app:layout_constraintStart_toEndOf="@+id/fourstaranchor"
        app:layout_constraintTop_toBottomOf="@+id/anchorevalutionbox"
        app:srcCompat="@drawable/star2" />

    <ImageButton
        android:id="@+id/backbutton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="16dp"
        android:background="#00FFFFFF"
        android:contentDescription="@string/back"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/back"
        tools:ignore="SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/playlistevalutionbox"
        android:layout_width="332dp"
        android:layout_height="120dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="16dp"
        android:background="@color/lightblue"
        android:cursorVisible="true"
        android:ellipsize="none"
        android:ems="10"
        android:fontFamily="monospace"
        android:gravity="start"
        android:hint="@string/playlisthint"
        android:inputType="textMultiLine|text"
        android:maxLines="5"
        android:textColor="@color/betweenblue"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.455"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TextContrastCheck"
        android:importantForAutofill="no" />

    <EditText
        android:id="@+id/anchorevalutionbox"
        android:layout_width="332dp"
        android:layout_height="120dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="16dp"
        android:background="@color/lightblue"
        android:cursorVisible="true"
        android:ems="10"
        android:fontFamily="monospace"
        android:gravity="start"
        android:hint="@string/anchorhint"
        android:inputType="textMultiLine|text"
        android:maxLines="5"
        android:textColor="@color/darkblue"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.455"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nickname"
        tools:ignore="TextContrastCheck"
        android:importantForAutofill="no" />

    <EditText
        android:id="@+id/nickname2"
        android:layout_width="170dp"
        android:layout_height="48dp"
        android:layout_marginTop="12dp"
        android:background="@color/lightblue"
        android:cursorVisible="true"
        android:ems="10"
        android:fontFamily="monospace"
        android:hint="@string/nickhint"
        android:inputType="textPersonName"
        android:maxLength="14"
        android:maxLines="1"
        android:textColor="@color/darkblue"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/anchorevalutionbox"
        app:layout_constraintTop_toBottomOf="@+id/anchorevalutionbox"
        tools:ignore="DuplicateSpeakableTextCheck,TextContrastCheck"
        android:importantForAutofill="no" />

    <ImageButton
        android:id="@+id/okbutton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="90dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="16dp"
        android:background="@color/transparent"
        android:contentDescription="@string/ok"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/nickname2"
        app:layout_constraintTop_toBottomOf="@+id/anchorevalutionbox"
        app:srcCompat="@drawable/done"
        app:tint="@color/green"
        tools:ignore="SpeakableTextPresentCheck,DuplicateSpeakableTextCheck" />

    <EditText
        android:id="@+id/nickname"
        android:layout_width="170dp"
        android:layout_height="48dp"
        android:layout_marginTop="12dp"
        android:background="@color/lightblue"
        android:cursorVisible="true"
        android:ems="10"
        android:fontFamily="monospace"
        android:hint="@string/nickhint"
        android:importantForAutofill="no"
        android:inputType="textPersonName"
        android:maxLength="14"
        android:maxLines="1"
        android:textColor="@color/betweenblue"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/playlistevalutionbox"
        app:layout_constraintTop_toBottomOf="@+id/playlistevalutionbox"
        tools:ignore="DuplicateSpeakableTextCheck,TextContrastCheck" />

    <ImageButton
        android:id="@+id/okbutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="90dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="16dp"
        android:background="@color/transparent"
        android:contentDescription="@string/ok"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.038"
        app:layout_constraintStart_toEndOf="@+id/nickname"
        app:layout_constraintTop_toBottomOf="@+id/playlistevalutionbox"
        app:srcCompat="@drawable/done"
        app:tint="@color/green"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/onestar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="23dp"
        android:layout_marginTop="12dp"
        android:contentDescription="@string/onestar"
        app:layout_constraintEnd_toStartOf="@+id/twostars"
        app:layout_constraintStart_toEndOf="@+id/nickname"
        app:layout_constraintTop_toBottomOf="@+id/playlistevalutionbox"
        app:srcCompat="@drawable/star1" />

    <ImageView
        android:id="@+id/twostars"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:contentDescription="@string/twostars"
        app:layout_constraintEnd_toStartOf="@+id/threestars"
        app:layout_constraintStart_toEndOf="@+id/onestar"
        app:layout_constraintTop_toBottomOf="@+id/playlistevalutionbox"
        app:srcCompat="@drawable/star2" />

    <ImageView
        android:id="@+id/threestars"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:contentDescription="@string/threestars"
        app:layout_constraintEnd_toStartOf="@+id/fourstars"
        app:layout_constraintStart_toEndOf="@+id/twostars"
        app:layout_constraintTop_toBottomOf="@+id/playlistevalutionbox"
        app:srcCompat="@drawable/star2" />

    <ImageView
        android:id="@+id/fourstars"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:contentDescription="@string/fourstars"
        app:layout_constraintEnd_toStartOf="@+id/fivestars"
        app:layout_constraintStart_toEndOf="@+id/threestars"
        app:layout_constraintTop_toBottomOf="@+id/playlistevalutionbox"
        app:srcCompat="@drawable/star2" />

    <ImageView
        android:id="@+id/fivestars"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:contentDescription="@string/fivestars"
        app:layout_constraintStart_toEndOf="@+id/fourstars"
        app:layout_constraintTop_toBottomOf="@+id/playlistevalutionbox"
        app:srcCompat="@drawable/star2" />

</androidx.constraintlayout.widget.ConstraintLayout>